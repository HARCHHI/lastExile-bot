{
  "env": {
    "node": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "rules": {
    "semi": [2, "always"],
    "comma-dangle": ["error", "never"],
    "func-names": ["error", "as-needed"],
    "no-restricted-syntax": 0,
    "no-param-reassign": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0
  }
}
